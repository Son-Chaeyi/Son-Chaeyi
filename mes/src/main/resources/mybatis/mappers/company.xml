	<insert id="insertCompany" parameterType="companyVO">
		<![CDATA[
			insert into company(comName, BRNum, CRNum, CEOName, 
								residentNum, zipCode, address, addressDetails,
								comNumber, faxNumber, businessCondition, businessType)
						values(#{comName}, #{BRNum}, #{CRNum}, #{CEOName}, 
								 #{residentNum}, #{zipCode}, #{address}, #{addressDetails},
								#{comNumber}, #{faxNumber}, #{businessCondition}, #{businessType})
		]]>
	</insert>
